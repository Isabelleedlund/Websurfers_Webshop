<%- include("../partial/header.ejs") %>


<div class="col-12 content slctn">
  <form class="col-12" action="/login" method="POST">
    <h1 class="title">LOG IN</h1>
    <div class="form-group">
      <input type="email" name="loginemail" class="inputlong slinput form-control" id="loginEmail"
        placeholder="Email" />
    </div>
    <div class="form-group">
      <input type="password" name="loginpassword" class="inputlong slinput form-control" id="loginPassword"
        placeholder="Password" />
    </div>
    <button type="submit" id="logInBtn" class="buttonblue">Log In</button>
  </form>
</div>

