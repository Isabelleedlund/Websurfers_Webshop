<%- include("./partial/header.ejs") %>

    <div class="col-12 content slctn">
        <form action="/signup" class="col-12" method="POST">
            <h1 class="title">SIGN UP</h1>
            <div class="form-group">
                <input type="email" name="email" class="inputlong slinput form-control" id="signUpEmail"
                    placeholder="Email" />
            </div>
            <div class="form-group">
                <input type="password" name="password" class="inputlong slinput form-control"
                    id="signUpPassword" placeholder="Password" />
            </div>
            <button type="submit" id="SignUpBtn" class="buttonblue">SIGN UP</button>
        </form>
    </div>

<%- include("./partial/footer.ejs") %>